<Window x:Class="WpfTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000" ResizeMode="NoResize">

    <Grid Name="MainGrid" Background="#FFFD8D41">
        <Canvas Name="LevelCanvas" Height="500" Width="450" Background="#FFA47342"/>
        <StackPanel Name="LevelGrid" Height="500" Width="450"/>
        <Button Name="Et" HorizontalAlignment="Center" Height="75" Margin="0,0,0,25" VerticalAlignment="Bottom" Width="75"/>
        <Button Name="To" HorizontalAlignment="Center" Height="75" Margin="0,25,0,0" VerticalAlignment="Top" Width="76" />
    </Grid>

</Window>
